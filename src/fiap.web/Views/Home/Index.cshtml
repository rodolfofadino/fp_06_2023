@model HomeViewModel






<section class="jumbotron text-center">
    <div class="container">
        <h1 class="jumbotron-heading">Fiap 2023</h1>
        <p class="lead text-muted">Conheca as novidades sobre os melhores destinos e esteja sempre por dentro das novidades.</p>
        <p>
            <a href="#" class="btn btn-primary my-2">Buscar passagens</a>
            <a href="#" class="btn btn-secondary my-2">Conheca nosso blog</a>
        </p>
    </div>
</section>

<div class="row">
    <div id="carroseul" class="col">
        <div class="jcarousel-wrapper">
            <div class="jcarousel">
                <ul>
                    <li>
                        <img class="carroseulimage" src="~/images/imagem-1.png" asp-append-version="true" />
                    </li>
                    <li>
                        <img class="carroseulimage" src="~/images/imagem-2.png" asp-append-version="true" />
                    </li>
                    <li>
                        <img class="carroseulimage" src="~/images/imagem-3.png" asp-append-version="true" />
                    </li>
                    <li>
                        <img class="carroseulimage" src="~/images/imagem-4.png" asp-append-version="true" />
                    </li>
                </ul>
            </div>
            <div>
                <a href="#" class="jcarousel-control-prev">&lsaquo;</a>
                <a href="#" class="jcarousel-control-next">&rsaquo;</a>
            </div>

        </div>
    </div>
</div>


<vc:noticias total="20" noticias-urgentes="false"></vc:noticias>




